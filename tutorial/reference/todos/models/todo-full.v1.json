{
  "title": "Todo Full",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
     "name": {
      "type": "string",
      "example": "Get groceries"
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "completed_at": {
      "type": "string",
      "nullable": true,
      "format": "date-time"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "updated_at": {
      "type": "string",
      "nullable": true,
      "format": "date-time",
      "readOnly": true
    },
    "user": {
      "$ref": "./user.v1.json"
    }
  },
  "required": [
    "id",
    "name",
    "completed",
    "completed_at",
    "created_at",
    "updated_at"
  ]
}
